{
  "name": "talentscout-client",
  "version": "0.1.0",
  "description": "招聘平台信息聚合工具客户端",
  "main": "electron/main.js",
  "private": true,
  "author": "GalaxySpace",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"npm run start\" \"electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:start": "electron ."
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.3.0",
    "axios": "^1.3.4",
    "electron-store": "^8.1.0",
    "playwright": "^1.31.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "build": {
    "appId": "com.galaxyspace.talentscout",
    "productName": "TalentScout",
    "mac": {
      "category": "public.app-category.business"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  }
}